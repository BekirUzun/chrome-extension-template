{
  "name": "Example Extension",
  "description": "Simple example extension that has popup menu",
  "version": "1.0",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_title": "Example popup title",
    "default_icon": "icon.png",
    "default_popup": "popup/popup.html"
  },
  "manifest_version": 2
}